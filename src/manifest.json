{
    "name": "Gradepeek",
    "description": "See Canvas without being in Canvas",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Gradepeek",
        "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "icons": {
        "16": "pic16.png",
        "32": "pic32.png",
        "48": "pic48.png",
        "128": "pic128.png"
    }
}
